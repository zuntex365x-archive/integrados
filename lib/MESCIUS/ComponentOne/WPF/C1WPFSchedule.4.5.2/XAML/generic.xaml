    <ResourceDictionary x:Key="{ComponentResourceKey TypeInTargetAssembly={x:Type c1sched:CalendarBase},    ResourceId=Office2016.Black}" Source="/C1.WPF.Schedule.4.5.2;component/themes/CalendarThemes/Office2016/Black.xaml" />
    <ResourceDictionary x:Key="{ComponentResourceKey TypeInTargetAssembly={x:Type c1sched:CalendarBase},    ResourceId=Office2016.White}" Source="/C1.WPF.Schedule.4.5.2;component/themes/CalendarThemes/Office2016/White.xaml" />
    <ResourceDictionary x:Key="{ComponentResourceKey TypeInTargetAssembly={x:Type c1sched:CalendarBase},    ResourceId=Office2016.DarkGray}" Source="/C1.WPF.Schedule.4.5.2;component/themes/CalendarThemes/Office2016/DarkGray.xaml" />

