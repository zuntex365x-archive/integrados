    <ResourceDictionary x:Key="{ComponentResourceKey TypeInTargetAssembly={x:Type c1sched:C1Scheduler},    ResourceId=Office2016.White}" Source="/C1.WPF.Schedule.4.5.2;component/themes/SchedulerThemes/Office2016/White.xaml" />
