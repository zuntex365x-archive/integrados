    <!-- Calendar default theme keys -->
	<ResourceDictionary x:Key="{ComponentResourceKey TypeInTargetAssembly={x:Type c1sched:CalendarBase},    ResourceId=Default}" Source="/C1.WPF.Schedule.4.5.2;component/themes/DefaultCalendar.xaml" />
	<ResourceDictionary x:Key="{ComponentResourceKey TypeInTargetAssembly={x:Type c1sched:CalendarBase},    ResourceId=Office2007.Blue}" Source="/C1.WPF.Schedule.4.5.2;component/themes/CalendarThemes/Office2007/Blue.xaml" />
	<ResourceDictionary x:Key="{ComponentResourceKey TypeInTargetAssembly={x:Type c1sched:CalendarBase},    ResourceId=Office2007.Black}" Source="/C1.WPF.Schedule.4.5.2;component/themes/CalendarThemes/Office2007/Black.xaml" />
