    <ResourceDictionary x:Key="{ComponentResourceKey TypeInTargetAssembly={x:Type c1sched:CalendarBase},    ResourceId=Office2010.Blue}" Source="/C1.WPF.Schedule.4.5.2;component/themes/CalendarThemes/Office2010/Blue.xaml" />
    <ResourceDictionary x:Key="{ComponentResourceKey TypeInTargetAssembly={x:Type c1sched:CalendarBase},    ResourceId=Office2010.Black}" Source="/C1.WPF.Schedule.4.5.2;component/themes/CalendarThemes/Office2010/Black.xaml" />
    <ResourceDictionary x:Key="{ComponentResourceKey TypeInTargetAssembly={x:Type c1sched:CalendarBase},    ResourceId=Office2010.Silver}" Source="/C1.WPF.Schedule.4.5.2;component/themes/CalendarThemes/Office2010/Silver.xaml" />

