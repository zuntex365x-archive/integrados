    <Style x:Key="ArrowStyle" TargetType="Path">
        <Setter Property="Fill">
            <Setter.Value>
                <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                    <GradientStop Color="#FF43637C" />
                    <GradientStop Color="#FF2C475C" Offset="1" />
                </LinearGradientBrush>
            </Setter.Value>
        </Setter>
        <Setter Property="Width" Value="5" />
        <Setter Property="Height" Value="3" />
    </Style>

    <Style TargetType="c1rtbtb:C1IncreaseFontSizeTool" BasedOn="{StaticResource C1.Silverlight.Toolbar.C1ToolbarButton}">
        <Setter Property="Height" Value="20" />
        <Setter Property="ContentTemplate">
            <Setter.Value>
                <DataTemplate>
                    <StackPanel Height="13" Orientation="Horizontal" HorizontalAlignment="Center" VerticalAlignment="Center">
                        <TextBlock FontFamily="Times New Roman" Foreground="Black" FontSize="15" Text="A" Margin="0 0 -2 0" TextWrapping="Wrap" FontWeight="Bold" TextAlignment="Center" HorizontalAlignment="Center" VerticalAlignment="Center" />
                        <Path HorizontalAlignment="Right" VerticalAlignment="Top" Stretch="Fill" StrokeThickness="1" Data="M0,0 L5,0 L2.5,-0.43323728 L2.5,-0.43751192 z" Style="{StaticResource ArrowStyle}" />
                    </StackPanel>
                </DataTemplate>
            </Setter.Value>
        </Setter>
    </Style>

    <Style TargetType="c1rtbtb:C1DecreaseFontSizeTool" BasedOn="{StaticResource C1.Silverlight.Toolbar.C1ToolbarButton}">
        <Setter Property="Height" Value="20" />
        <Setter Property="ContentTemplate">
            <Setter.Value>
                <DataTemplate>
                    <StackPanel Height="13" Width="17" Orientation="Horizontal" HorizontalAlignment="Left" VerticalAlignment="Bottom">
                        <TextBlock Text="A" Foreground="Black" TextWrapping="Wrap" FontFamily="Times New Roman" FontSize="9" FontWeight="Bold" TextAlignment="Center" Margin="4,3,-1,0" />
                        <Path HorizontalAlignment="Right" VerticalAlignment="Top" Stretch="Fill" StrokeThickness="1" Data="M0,0 L5,0 L2.5,3 L2.5,3 z" Margin="0" Style="{StaticResource ArrowStyle}" />
                    </StackPanel>
                </DataTemplate>
            </Setter.Value>
        </Setter>
    </Style>

    <Style TargetType="c1rtbtb:C1BoldTool" BasedOn="{StaticResource C1.Silverlight.Toolbar.C1ToolbarToggleButton}">
        <Setter Property="ContentTemplate">
            <Setter.Value>
                <DataTemplate>
                    <TextBlock Foreground="Black" Text="{Binding Path=Font_BoldInitial, Source={StaticResource RichTextBoxToolbarResources}}" TextWrapping="Wrap" FontFamily="Times New Roman" FontSize="13" FontWeight="Bold" TextAlignment="Center" />
                </DataTemplate>
            </Setter.Value>
        </Setter>
    </Style>

    <Style TargetType="c1rtbtb:C1ItalicTool" BasedOn="{StaticResource C1.Silverlight.Toolbar.C1ToolbarToggleButton}">
        <Setter Property="ContentTemplate">
            <Setter.Value>
                <DataTemplate>
                    <TextBlock Foreground="Black" Text="{Binding Path=Font_ItalicInitial, Source={StaticResource RichTextBoxToolbarResources}}" TextWrapping="Wrap" FontFamily="Times New Roman" FontSize="13" FontWeight="Normal" FontStyle="Italic" TextAlignment="Left" Margin="4 0 6 0" />
                </DataTemplate>
            </Setter.Value>
        </Setter>
    </Style>

    <Style TargetType="c1rtbtb:C1UnderlineTool" BasedOn="{StaticResource C1.Silverlight.Toolbar.C1ToolbarToggleButton}">
        <Setter Property="ContentTemplate">
            <Setter.Value>
                <DataTemplate>
                    <TextBlock Foreground="Black" Text="{Binding Path=Font_UnderlineInitial, Source={StaticResource RichTextBoxToolbarResources}}" TextWrapping="Wrap" FontFamily="Times New Roman" FontSize="13" FontWeight="Normal" TextDecorations="Underline" TextAlignment="Left" Margin="4 0 3 0" />
                </DataTemplate>
            </Setter.Value>
        </Setter>
    </Style>

    <Style TargetType="c1rtbtb:C1StrikethroughTool" BasedOn="{StaticResource C1.Silverlight.Toolbar.C1ToolbarToggleButton}">
        <Setter Property="ContentTemplate">
            <Setter.Value>
                <DataTemplate>
