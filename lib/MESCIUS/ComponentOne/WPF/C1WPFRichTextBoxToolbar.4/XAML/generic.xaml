    <c1rtbtb_Resources:C1_Silverlight_RichTextBox_Toolbar x:Key="RichTextBoxToolbarResources" />

    <Style TargetType="c1rtbtb:C1RichTextBoxToolbar">
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="c1rtbtb:C1RichTextBoxToolbar">
                    <c1tb:C1Toolbar>
                        <c1tb:C1ToolbarTabItem Header="{Binding Path=HomeTab_Header, Source={StaticResource RichTextBoxToolbarResources}}">

                            <c1tb:C1ToolbarGroup Header="{Binding Path=Edit_Title, Source={StaticResource RichTextBoxToolbarResources}}">
                                <c1rtbtb:C1PasteLargeTool ToolTipService.ToolTip="{Binding Tools_Paste, Source={StaticResource RichTextBoxToolbarResources}}" />
                                <c1rtbtb:C1CutTool ToolTipService.ToolTip="{Binding Tools_Cut, Source={StaticResource RichTextBoxToolbarResources}}" />
                                <c1rtbtb:C1UndoTool ToolTipService.ToolTip="{Binding Tools_Undo, Source={StaticResource RichTextBoxToolbarResources}}" />
                                <c1:C1Separator Width="0" Height="20" />
                                <c1rtbtb:C1CopyTool ToolTipService.ToolTip="{Binding Tools_Copy, Source={StaticResource RichTextBoxToolbarResources}}" />
                                <c1rtbtb:C1RedoTool ToolTipService.ToolTip="{Binding Tools_Redo, Source={StaticResource RichTextBoxToolbarResources}}" />
                                <c1rtbtb:C1FormatPainterTool ToolTipService.ToolTip="{Binding FormatPainter, Source={StaticResource RichTextBoxToolbarResources}}" />

                            </c1tb:C1ToolbarGroup>

                            <c1tb:C1ToolbarGroup Header="{Binding Path=Font_Title, Source={StaticResource RichTextBoxToolbarResources}}" VerticalContentAlignment="Center">

                                <c1tb:C1ToolbarStrip Overflow="None">

                                    <c1rtbtb:C1FontFamilyTool ToolTipService.ToolTip="{Binding Font_Font, Source={StaticResource RichTextBoxToolbarResources}}" />
                                    <c1:C1Separator />
                                    <c1rtbtb:C1FontSizeTool ToolTipService.ToolTip="{Binding Font_FontSize, Source={StaticResource RichTextBoxToolbarResources}}" />
                                    <c1:C1Separator />
                                    <c1rtbtb:C1IncreaseFontSizeTool ToolTipService.ToolTip="{Binding Font_GrowFont, Source={StaticResource RichTextBoxToolbarResources}}" />
                                    <c1rtbtb:C1DecreaseFontSizeTool ToolTipService.ToolTip="{Binding Font_ShrinkFont , Source={StaticResource RichTextBoxToolbarResources}}" />

								</c1tb:C1ToolbarStrip>

                                <c1tb:C1ToolbarStrip Overflow="None">

                                    <c1rtbtb:C1BoldTool ToolTipService.ToolTip="{Binding Font_Bold, Source={StaticResource RichTextBoxToolbarResources}}" />
                                    <c1rtbtb:C1ItalicTool ToolTipService.ToolTip="{Binding Font_Italic, Source={StaticResource RichTextBoxToolbarResources}}" />
                                    <c1rtbtb:C1UnderlineTool ToolTipService.ToolTip="{Binding Font_Underline, Source={StaticResource RichTextBoxToolbarResources}}" />
                                    <c1rtbtb:C1StrikethroughTool ToolTipService.ToolTip="{Binding Font_Strikethrough, Source={StaticResource RichTextBoxToolbarResources}}" />
                                    <c1rtbtb:C1ChangeCaseTool ToolTipService.ToolTip="{Binding Font_ChangeCase, Source={StaticResource RichTextBoxToolbarResources}}" />
                                    <c1rtbtb:C1SuperscriptTool ToolTipService.ToolTip="{Binding Font_Superscript, Source={StaticResource RichTextBoxToolbarResources}}" />
                                    <c1rtbtb:C1SubscriptTool ToolTipService.ToolTip="{Binding Font_Subscript, Source={StaticResource RichTextBoxToolbarResources}}" />
                                    <c1:C1Separator />
                                    <c1rtbtb:C1FontColorTool ToolTipService.ToolTip="{Binding Font_FontColor, Source={StaticResource RichTextBoxToolbarResources}}" />
                                    <c1rtbtb:C1TextHighlightTool ToolTipService.ToolTip="{Binding Font_TextHighlightColor, Source={StaticResource RichTextBoxToolbarResources}}" />

                                </c1tb:C1ToolbarStrip>

                            </c1tb:C1ToolbarGroup>

                            <c1tb:C1ToolbarGroup Header="{Binding Path=Paragraph_Title, Source={StaticResource RichTextBoxToolbarResources}}" VerticalContentAlignment="Center">

                                <c1tb:C1ToolbarStrip Overflow="None" Margin="0 4 0 3">

                                    <c1rtbtb:C1LeftAlignTool ToolTipService.ToolTip="{Binding Paragraph_AlignTextLeft, Source={StaticResource RichTextBoxToolbarResources}}" />
                                    <c1rtbtb:C1CenterAlignTool ToolTipService.ToolTip="{Binding Paragraph_AlignTextCenter, Source={StaticResource RichTextBoxToolbarResources}}" />
                                    <c1rtbtb:C1RightAlignTool ToolTipService.ToolTip="{Binding Paragraph_AlignTextRight, Source={StaticResource RichTextBoxToolbarResources}}" />
                                    <c1rtbtb:C1JustifyTool ToolTipService.ToolTip="{Binding Paragraph_Justify, Source={StaticResource RichTextBoxToolbarResources}}" />
                                    <c1:C1Separator />
                                    <c1rtbtb:C1BulletsTool ToolTipService.ToolTip="{Binding Paragraph_Bullets, Source={StaticResource RichTextBoxToolbarResources}}" />
                                    <c1rtbtb:C1NumberingTool ToolTipService.ToolTip="{Binding Paragraph_Numbering, Source={StaticResource RichTextBoxToolbarResources}}" />
                                    <c1:C1Separator />
                                    <c1rtbtb:C1TextWrappingTool ToolTipService.ToolTip="{Binding Paragraph_TextWrapping, Source={StaticResource RichTextBoxToolbarResources}}" />

                                </c1tb:C1ToolbarStrip>

                                <c1tb:C1ToolbarStrip Overflow="None">

                                    <c1rtbtb:C1ParagraphColorTool ToolTipService.ToolTip="{Binding Paragraph_ParagraphColor, Source={StaticResource RichTextBoxToolbarResources}}" />
                                    <c1rtbtb:C1BorderTool ToolTipService.ToolTip="{Binding Tools_BorderTool, Source={StaticResource RichTextBoxToolbarResources}}" />
                                    <c1:C1Separator />
                                    <c1rtbtb:C1MarginTool ToolTipService.ToolTip="{Binding Paragraph_Margin, Source={StaticResource RichTextBoxToolbarResources}}" />
                                    <c1rtbtb:C1PaddingTool ToolTipService.ToolTip="{Binding Paragraph_Padding, Source={StaticResource RichTextBoxToolbarResources}}" />

                                </c1tb:C1ToolbarStrip>

                            </c1tb:C1ToolbarGroup>

                            <c1tb:C1ToolbarGroup Header="{Binding Path=Insert_Title, Source={StaticResource RichTextBoxToolbarResources}}">

                                <c1rtbtb:C1InsertImageLargeTool ToolTipService.ToolTip="{Binding Insert_InsertImage, Source={StaticResource RichTextBoxToolbarResources}}" />
                                  <c1rtbtb:C1InsertSymbolTool ToolTipService.ToolTip="{Binding Insert_InsertSymbol, Source={StaticResource RichTextBoxToolbarResources}}" />
                                <c1rtbtb:C1InsertHyperlinkTool ToolTipService.ToolTip="{Binding Insert_InsertHyperlink, Source={StaticResource RichTextBoxToolbarResources}}" />
                                <c1rtbtb:C1RemoveHyperlinkTool ToolTipService.ToolTip="{Binding Insert_RemoveHyperlink, Source={StaticResource RichTextBoxToolbarResources}}" />

                            </c1tb:C1ToolbarGroup>

                            <c1tb:C1ToolbarGroup Header="{Binding Path=Tools_Title, Source={StaticResource RichTextBoxToolbarResources}}">

                                <c1tb:C1ToolbarGroup.GroupSizeDefinitions>
                                    <c1tb:C1ToolbarGroupSizeDefinition>
                                        <c1tb:C1ToolbarControlSize>Large</c1tb:C1ToolbarControlSize>
                                        <c1tb:C1ToolbarControlSize>Medium</c1tb:C1ToolbarControlSize>
                                    </c1tb:C1ToolbarGroupSizeDefinition>
                                </c1tb:C1ToolbarGroup.GroupSizeDefinitions>

                                <c1rtbtb:C1SpellCheckLargeTool ToolTipService.ToolTip="{Binding Tools_SpellCheck, Source={StaticResource RichTextBoxToolbarResources}}" />
                                <c1rtbtb:C1FindAndReplaceLargeTool ToolTipService.ToolTip="{Binding Tools_FindAndReplace, Source={StaticResource RichTextBoxToolbarResources}}" />

                            </c1tb:C1ToolbarGroup>

                        </c1tb:C1ToolbarTabItem>

                        <c1tb:C1ToolbarTabItem Header="{Binding Path=TablesTab_Header, Source={StaticResource RichTextBoxToolbarResources}}">

                            <c1tb:C1ToolbarGroup Header="{Binding Path=Table_Title, Source={StaticResource RichTextBoxToolbarResources}}">

                                <c1tb:C1ToolbarGroup.GroupSizeDefinitions>
                                    <c1tb:C1ToolbarGroupSizeDefinition>
                                        <c1tb:C1ToolbarControlSize>Large</c1tb:C1ToolbarControlSize>
                                    </c1tb:C1ToolbarGroupSizeDefinition>
                                </c1tb:C1ToolbarGroup.GroupSizeDefinitions>

                                <c1rtbtb:C1InsertTableTool ToolTipService.ToolTip="{Binding Tools_InsertTable, Source={StaticResource RichTextBoxToolbarResources}}" />
                                <c1rtbtb:C1SelectTableTool ToolTipService.ToolTip="{Binding Tools_SelectTable, Source={StaticResource RichTextBoxToolbarResources}}" />
                                <c1rtbtb:C1ShowGridlinesTool ToolTipService.ToolTip="{Binding Tools_ShowGridlines, Source={StaticResource RichTextBoxToolbarResources}}" />
                                <c1rtbtb:C1TableWidthTool VerticalAlignment="Top" />

                            </c1tb:C1ToolbarGroup>

                            <c1tb:C1ToolbarGroup Header="{Binding Path=RowsAndColumns_Title, Source={StaticResource RichTextBoxToolbarResources}}">

                                <c1tb:C1ToolbarGroup.GroupSizeDefinitions>
                                    <c1tb:C1ToolbarGroupSizeDefinition>
                                        <c1tb:C1ToolbarControlSize>Large</c1tb:C1ToolbarControlSize>
                                    </c1tb:C1ToolbarGroupSizeDefinition>
                                </c1tb:C1ToolbarGroup.GroupSizeDefinitions>

                                <c1rtbtb:C1DeleteTableTool ToolTipService.ToolTip="{Binding Tools_DeleteTable, Source={StaticResource RichTextBoxToolbarResources}}" />
                                <c1rtbtb:C1InsertColumnsLeftTool ToolTipService.ToolTip="{Binding Tools_InsertColumnsLeft, Source={StaticResource RichTextBoxToolbarResources}}" />
                                <c1rtbtb:C1InsertColumnsRightTool ToolTipService.ToolTip="{Binding Tools_InsertColumnsRight, Source={StaticResource RichTextBoxToolbarResources}}" />
                                <c1rtbtb:C1InsertRowsAboveTool ToolTipService.ToolTip="{Binding Tools_InsertRowsAbove, Source={StaticResource RichTextBoxToolbarResources}}" />
                                <c1rtbtb:C1InsertRowsBelowTool ToolTipService.ToolTip="{Binding Tools_InsertRowsBelow, Source={StaticResource RichTextBoxToolbarResources}}" />
                            </c1tb:C1ToolbarGroup>

                            <c1tb:C1ToolbarGroup Header="{Binding Path=Merge_Title, Source={StaticResource RichTextBoxToolbarResources}}">

                                <c1tb:C1ToolbarGroup.GroupSizeDefinitions>
                                    <c1tb:C1ToolbarGroupSizeDefinition>
                                        <c1tb:C1ToolbarControlSize>Large</c1tb:C1ToolbarControlSize>
                                    </c1tb:C1ToolbarGroupSizeDefinition>
                                </c1tb:C1ToolbarGroup.GroupSizeDefinitions>

                                <c1rtbtb:C1MergeCellsTool ToolTipService.ToolTip="{Binding Tools_MergeCells, Source={StaticResource RichTextBoxToolbarResources}}" />
                                <c1rtbtb:C1UnmergeCellTool ToolTipService.ToolTip="{Binding Tools_UnmergeCell, Source={StaticResource RichTextBoxToolbarResources}}" />

                            </c1tb:C1ToolbarGroup>

                            <c1tb:C1ToolbarGroup VerticalContentAlignment="Center" Header="{Binding Path=Cell_Title, Source={StaticResource RichTextBoxToolbarResources}}">

                                <c1tb:C1ToolbarGroup.GroupSizeDefinitions>
                                    <c1tb:C1ToolbarGroupSizeDefinition>
                                        <c1tb:C1ToolbarControlSize>Small</c1tb:C1ToolbarControlSize>
                                    </c1tb:C1ToolbarGroupSizeDefinition>
                                </c1tb:C1ToolbarGroup.GroupSizeDefinitions>

                                <c1rtbtb:C1ColumnWidthTool />
                                <c1rtbtb:C1RowHeightTool />
                                <c1:C1Separator Background="Transparent" Width="130" />
                                <c1:C1Separator Orientation="Vertical" Height="68" />
                                <c1:C1Separator Width="0" Height="0" />
                                <c1:C1Separator Width="0" Height="0" />
                                <c1rtbtb:C1TopAlignTool ToolTipService.ToolTip="{Binding Tools_TopAlign, Source={StaticResource RichTextBoxToolbarResources}}" />
                                <c1rtbtb:C1VerticalCenterAlignTool ToolTipService.ToolTip="{Binding Tools_VerticalCenterAlign, Source={StaticResource RichTextBoxToolbarResources}}" />
                                <c1rtbtb:C1BottomAlignTool ToolTipService.ToolTip="{Binding Tools_BottomAlign, Source={StaticResource RichTextBoxToolbarResources}}" />

                            </c1tb:C1ToolbarGroup>

                        </c1tb:C1ToolbarTabItem>

                        <c1tb:C1ToolbarTabItem Header="{Binding Path=ViewTab_Header, Source={StaticResource RichTextBoxToolbarResources}}">

                            <c1tb:C1ToolbarGroup Header="{Binding Path=View_DocumentViews, Source={StaticResource RichTextBoxToolbarResources}}">
                                
                                <c1tb:C1ToolbarGroup.GroupSizeDefinitions>
                                    <c1tb:C1ToolbarGroupSizeDefinition>
                                        <c1tb:C1ToolbarControlSize>Large</c1tb:C1ToolbarControlSize>
                                    </c1tb:C1ToolbarGroupSizeDefinition>
                                </c1tb:C1ToolbarGroup.GroupSizeDefinitions>
                                
                                <c1rtbtb:C1PrintViewLargeTool ToolTipService.ToolTip="{Binding View_PrintLayout, Source={StaticResource RichTextBoxToolbarResources}}" />
                                <c1rtbtb:C1DraftViewLargeTool ToolTipService.ToolTip="{Binding View_Draft, Source={StaticResource RichTextBoxToolbarResources}}" />
                                
                            </c1tb:C1ToolbarGroup>

                            <c1tb:C1ToolbarGroup MaxHeight="100" Header="{Binding Path=View_Zoom, Source={StaticResource RichTextBoxToolbarResources}}">
                                <c1tb:C1ToolbarGroup.GroupSizeDefinitions>
                                    <c1tb:C1ToolbarGroupSizeDefinition>
                                        <c1tb:C1ToolbarControlSize>Large</c1tb:C1ToolbarControlSize>
                                        <c1tb:C1ToolbarControlSize>Large</c1tb:C1ToolbarControlSize>
                                        <c1tb:C1ToolbarControlSize>Large</c1tb:C1ToolbarControlSize>
                                        <c1tb:C1ToolbarControlSize>Small</c1tb:C1ToolbarControlSize>
                                        <c1tb:C1ToolbarControlSize>Small</c1tb:C1ToolbarControlSize>
                                        <c1tb:C1ToolbarControlSize>Small</c1tb:C1ToolbarControlSize>
                                    </c1tb:C1ToolbarGroupSizeDefinition>
                                </c1tb:C1ToolbarGroup.GroupSizeDefinitions>
                                <c1rtbtb:C1ZoomInTool ToolTipService.ToolTip="{Binding Tools_ZoomIn, Source={StaticResource RichTextBoxToolbarResources}}" />
                                <c1rtbtb:C1ZoomOutTool ToolTipService.ToolTip="{Binding Tools_ZoomOut, Source={StaticResource RichTextBoxToolbarResources}}" />
                                <c1rtbtb:C1Zoom100PercentTool ToolTipService.ToolTip="{Binding Tools_Zoom100PercentTool, Source={StaticResource RichTextBoxToolbarResources}}" />
                                <c1rtbtb:C1OnePageTool ToolTipService.ToolTip="{Binding View_OnePage, Source={StaticResource RichTextBoxToolbarResources}}" HorizontalAlignment="Left" />
                                <c1rtbtb:C1TwoPagesTool ToolTipService.ToolTip="{Binding View_TwoPages, Source={StaticResource RichTextBoxToolbarResources}}" HorizontalAlignment="Left" />
                                <c1rtbtb:C1FitWidthTool ToolTipService.ToolTip="{Binding View_FitWidth, Source={StaticResource RichTextBoxToolbarResources}}" HorizontalAlignment="Left" />
                            </c1tb:C1ToolbarGroup>
							<c1tb:C1ToolbarGroup MaxHeight="100" Header="{Binding Path=LineNumber, Source={StaticResource RichTextBoxToolbarResources}}">
								<c1rtbtb:C1LineNumberTool ToolTipService.ToolTip="{Binding LineNumber, Source={StaticResource RichTextBoxToolbarResources}}" />
                            </c1tb:C1ToolbarGroup>

						</c1tb:C1ToolbarTabItem>
                    </c1tb:C1Toolbar>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>

    <Style TargetType="ContentControl" x:Key="DropDownContent">
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="ContentControl">
                    <Grid>
                        <Rectangle RadiusX="2" RadiusY="2" Fill="{TemplateBinding Background}" />
                        <c1:SmallShapePresenter>
                            <Path Margin="1,4,1,1" VerticalAlignment="Stretch" Stretch="Fill" StrokeThickness="1" Data="M97.5,3.02317 L97.5,46.713001 C97.5,46.713001 97.510521,48.713001 97.510521,48.713001 L0.51441199,48.713001 C0.50036031,47.842781 0.49999982,46.950779 0.5,46.056 C0.49999982,20.896116 40.5,2.9937413 97.5,3.02317 z" d:LayoutOverrides="GridBox" Opacity="0.85">
                                <Path.Fill>
                                    <RadialGradientBrush GradientOrigin="0.7,0.7">
                                        <RadialGradientBrush.RelativeTransform>
                                            <TransformGroup>
                                                <ScaleTransform CenterX="0.5" CenterY="0.5" ScaleY="1.887" ScaleX="1.78" />
                                                <SkewTransform CenterX="0.5" CenterY="0.5" />
                                                <RotateTransform CenterX="0.5" CenterY="0.5" />
                                                <TranslateTransform Y="0.4" X="0.4" />
                                            </TransformGroup>
                                        </RadialGradientBrush.RelativeTransform>
                                        <GradientStop Color="#07000000" />
                                        <GradientStop Color="#0F000000" Offset="1" />
