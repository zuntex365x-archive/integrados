                    </Grid>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>

    <Style TargetType="c1rtbtb:C1FontFamilyTool" BasedOn="{StaticResource C1ComboBoxStyle}">
        <Setter Property="Width" Value="111" />
        <Setter Property="Height" Value="22" />
        <Setter Property="Margin" Value="0 4 0 3" />
        <Setter Property="MouseOverBrush" Value="AliceBlue"/>
        <Setter Property="ItemTemplate">
            <Setter.Value>
                <DataTemplate>
                    <Grid>
                        <Grid.Resources>
                            <c1rtbtb:FontFamilyConverter x:Key="converter" />
                        </Grid.Resources>
                        <TextBlock Text="{Binding Converter={StaticResource converter}}" FontFamily="{Binding}" VerticalAlignment="Center" Margin="2" />
                    </Grid>
                </DataTemplate>
            </Setter.Value>
        </Setter>
    </Style>

    <Style TargetType="c1rtbtb:C1FontSizeTool" BasedOn="{StaticResource C1ComboBoxStyle}">
        <Setter Property="Width" Value="40" />
        <Setter Property="Height" Value="22" />
        <Setter Property="IsEditable" Value="True" />
        <Setter Property="Margin" Value="0 4 0 3" />
    </Style>

