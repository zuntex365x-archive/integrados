    <Style x:Key="SimplifiedThicknessTool" TargetType="c1rtbtb:C1SimplifiedThicknessTool" BasedOn="{StaticResource C1.Silverlight.Toolbar.C1ToolbarDropdownItem}">
        <Setter Property="AutoClose" Value="true" />
        <Setter Property="PopupContentTemplate">
            <Setter.Value>
                <DataTemplate>
                        <c1rtbtb:C1ThicknessPicker Thickness="{Binding Thickness, Mode=TwoWay}" />
                </DataTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <Style TargetType="c1rtbtb:C1SimpilfiedMarginTool" BasedOn="{StaticResource SimplifiedThicknessTool}" />
