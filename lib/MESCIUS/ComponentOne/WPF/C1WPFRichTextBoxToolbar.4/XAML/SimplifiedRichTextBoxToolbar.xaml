    <Style TargetType="c1rtbtb:C1SimplifiedTextHighlightTool" BasedOn="{StaticResource SimplifiedColorTool}" />

    <Style TargetType="c1rtbtb:C1SimplifiedFindAndReplaceTool" BasedOn="{StaticResource C1.Silverlight.Toolbar.C1ToolbarDropdownItem}">
        <Setter Property="PopupContentTemplate">
            <Setter.Value>
                <DataTemplate>
                    <c1rtbtb:C1FindAndReplace RichTextBox="{Binding RichTextBox}" DropDown="{Binding DropDown}" Margin="5" />
                </DataTemplate>
            </Setter.Value>
        </Setter>
    </Style>

    <Style TargetType="c1rtbtb:C1SimplifiedParagraphColorTool" BasedOn="{StaticResource SimplifiedColorTool}">
