    <Style TargetType="c1rtbtb:C1SimplifiedInsertSymbolTool" BasedOn="{StaticResource C1.Silverlight.Toolbar.C1ToolbarDropdownItem}">
        <Setter Property="AutoClose" Value="true" />
        <Setter Property="PopupContentTemplate">
            <Setter.Value>
                <DataTemplate>
                    <ListBox ItemsSource="{Binding Items}" SelectedItem="{Binding Selection, Mode=TwoWay}">
                        <ListBox.Template>
                            <ControlTemplate TargetType="ListBox">
                                <Grid>
                                    <ContentControl>
                                        <ItemsPresenter />
                                    </ContentControl>
                                </Grid>
                            </ControlTemplate>
                        </ListBox.Template>
                        <ListBox.ItemsPanel>
                            <ItemsPanelTemplate>
                                <c1:C1WrapPanel Width="120" Margin="4" />
                            </ItemsPanelTemplate>
                        </ListBox.ItemsPanel>
                        <ListBox.ItemTemplate>
                            <DataTemplate>
                                <Grid Width="20" Height="20">
                                    <TextBlock Text="{Binding}" HorizontalAlignment="Center" VerticalAlignment="Center" FontFamily="Times New Roman" FontSize="14" />
                                </Grid>
                            </DataTemplate>
                        </ListBox.ItemTemplate>
                        <ListBox.ItemContainerStyle>
                            <Style TargetType="ListBoxItem">
                                <Setter Property="Padding" Value="0" />
                                <Setter Property="Margin" Value="0" />
                            </Style>
                        </ListBox.ItemContainerStyle>
                    </ListBox>
                </DataTemplate>
            </Setter.Value>
        </Setter>
    </Style>
